{
  "name": "uf-std",
  "description": "A social blogging platform for UF students.",
  "version": "0.0.0",
  "homepage": "",
  "main": "src",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Chris Anderson",
    "email": "chris.scott.anders@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "^11.0.0",
    "npm": ">= 3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*"
    ],
    "events": {
      "restart": "npx webpack --config webpack.config.js"
    }
  },
  "scripts": {
    "test": "npm run eslint && NODE_ENV= npm run mocha",
    "eslint": "eslint src/server/. test/. --config .eslintrc.json",
    "dev": "webpack && nodemon -e vue src/server/",
    "start": "node src/server/",
    "mocha": "mocha test/ --recursive --exit",
    "build": "webpack"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.13",
    "@feathersjs/authentication-jwt": "^2.0.7",
    "@feathersjs/authentication-local": "^1.2.7",
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "css-loader": "^1.0.1",
    "feathers-mongoose": "^6.2.0",
    "helmet": "^3.15.0",
    "localstorage-memory": "^1.0.3",
    "mongodb-core": "^3.1.9",
    "mongoose": "^5.3.13",
    "serve-favicon": "^2.5.0",
    "superagent": "^4.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.2",
    "winston": "^3.1.0"
  },
