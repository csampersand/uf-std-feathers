    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@feathersjs/authentication-client": "^1.0.8",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/rest-client": "^1.4.5",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
